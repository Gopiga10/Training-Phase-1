   "source": [
    "import time  \n",
    "def timer_decorator(func):\n",
    "    def wrapper():\n",
    "        start = time.time()\n",
    "        func()\n",
    "        print(f\"Elapsed time: {time.time() - start:.4f} seconds\")\n",
    "    return wrapper  \n",
    "@timer_decorator\n",
    "def sample_function():\n",
    "    time.sleep(1)  \n",
    "\n",
    "sample_function()"
   ]
